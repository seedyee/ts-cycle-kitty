{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "outDir": "build/lib/client",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["dom", "es6", "es7"]
  },
  "formatCodeOptions": {
    "indentSize": 2,
    "tabSize": 2
  },
  "files": [
    "src/index.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
  }
}
